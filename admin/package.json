    "nprogress": "0.2.0",
    "path-to-regexp": "6.2.0",
    "relation-graph": "^1.1.0",
    "vue": "2.6.12",
    "vue-count-to": "^1.0.13",
    "vue-router": "3.5.1",
    "vue-super-flow": "^1.3.6",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/register": "7.13.16",
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-unit-jest": "4.5.12",
    "@vue/cli-service": "4.5.12",
