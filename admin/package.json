    "mockjs": "1.0.1-beta3",
    "node-sass": "4.12.0",
    "runjs": "^4.3.2",
    "sass-loader": "10.0.0",
    "script-ext-html-webpack-plugin": "2.1.5",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "6.0.5",
